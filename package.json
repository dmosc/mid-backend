{
  "name": "mid-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "babel-node src",
    "dev": "nodemon src --exec babel-node",
    "test": "jest --passWithNoTests --config ./jest.config.js --coverage",
    "db:seed": "yarn db:drop && babel-node src/db/scripts/seed.js",
    "db:drop": "babel-node src/db/scripts/drop.js",
    "lint": "eslint",
    "prettier": "prettier --write \"**/*.{js,graphql,json,css}\"",
    "format": "prettier --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.5",
    "@graphql-tools/schema": "^7.1.5",
    "@graphql-tools/utils": "^7.10.0",
    "apollo-server-express": "^2.25.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "dotenv": "^10.0.0",
    "eslint-plugin-jest": "^24.3.6",
    "express": "^4.17.1",
    "generate-password": "^1.6.0",
    "graphql": "15.5.1",
    "graphql-upload": "^12.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.1",
    "mongoose-delete": "^0.5.3",
    "signale": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "@shelf/jest-mongodb": "^1.2.5",
    "apollo-server-testing": "^2.25.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.0",
    "jest": "^27.0.6",
    "mongodb-memory-server": "^7.0.0",
    "nodemon": "^2.0.9",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1"
  },
  "engines": {
    "node": "12.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format . && git add . && pretty-quick --staged",
      "pre-push": "yarn lint ."
    }
  }
}
